package gui_control.MovingBarSideModel;


public class MovingBarSideModel {
	String name;
	String quantityColor;
	String referenceColor;
	String mode;
	int referenceValue;
	int stepSize;
	
	public MovingBarSideModel(String name) {
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getQuantityColor() {
		return quantityColor;
	}

	public void setQuantityColor(String quantityColor) {
		this.quantityColor = quantityColor;
	}

	public String getReferenceColor() {
		return referenceColor;
	}

	public void setReferenceColor(String referenceColor) {
		this.referenceColor = referenceColor;
	}

	public String getMode() {
		return mode;
	}

	public void setMode(String mode) {
		this.mode = mode;
	}

	public int getReferenceValue() {
		return referenceValue;
	}

	public void setReferenceValue(int referenceValue) {
		this.referenceValue = referenceValue;
	}

	public int getStepSize() {
		return stepSize;
	}

	public void setStepSize(int stepSize) {
		this.stepSize = stepSize;
	}
	
	
	
	
	
}
